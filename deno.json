{
  "license": "GPL-3.0-or-later",
  "workspace": [
    "./25519",
    "./aead",
    "./base",
    "./csv",
    "./fuzz",
    "./hash",
    "./inline",
    "./json",
    "./query",
    "./result",
    "./signal",
    "./state",
    "./url",
    "./weight",
  ],
  "compilerOptions": {
    "exactOptionalPropertyTypes": true
  },
  "lint": {
    "rules": {
      "include": [
        "default-param-last",
        "explicit-module-boundary-types",
        "no-inferrable-types",
        "no-self-compare",
        "no-sparse-arrays",
        "no-useless-rename",
        "prefer-ascii",
        "verbatim-module-syntax"
      ],
      "exclude": [
        "ban-types",
        "no-cond-assign",
        "no-control-regex",
        "no-explicit-any"
      ]
    }
  },
  "imports": {
    "@deno/dnt": "jsr:@deno/dnt@^0.42.3",
    "@std/assert": "jsr:@std/assert@^1.0.14",
    "fast-check": "npm:fast-check@^4.3.0"
  }
}
